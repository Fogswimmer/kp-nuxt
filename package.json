{
  "name": "kp-nuxt",
  "version": "1.0.0",
  "description": "My cool Nuxt + Electron app",
  "author": "Andrey Dyakov",
  "private": false,
 
  "main": "dist-electron/main.js",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:error": "eslint --quiet --ext .jsx,.js,.tsx,.ts src/**/*",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "build:electron": "nuxt generate && tsc electron/main.ts --outDir dist-electron",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
"build": {
  "appId": "com.example.kpnuxt",
  "files": [
    "dist-electron/**/*",
    ".output/public/**/*"
  ],
  "extraResources": [
    ".output/public"
  ],
  "linux": {
    "target": ["AppImage"],
    "category": "Utility"
  }
},
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/icon": "^1.12.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/test-utils": "^3.17.2",
    "@nuxt/ui": "^2.22.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^9.5.3",
    "@pinia/nuxt": "^0.9.0",
    "@pinia/testing": "^0.1.7",
    "@testdeck/mocha": "^0.3.3",
    "@types/chai": "^5.2.1",
    "@types/node": "^22.15.2",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "electron": "^35.2.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.19.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.32.0",
    "happy-dom": "^17.4.4",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.16.2",
    "nuxt-auth-utils": "0.5.14",
    "nuxt-electron": "^0.7.0",
    "nyc": "^17.1.0",
    "playwright-core": "^1.52.0",
    "prettier": "^3.4.2",
    "prettier-eslint": "^16.4.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "sass-embedded": "^1.87.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-eslint2": "^5.0.3",
    "vite-plugin-vuetify": "^2.1.1",
    "vitest": "^3.1.2",
    "vue-i18n": "^11.1.3",
    "vuetify": "^3.8.2",
    "vuetify-nuxt-module": "^0.18.6",
    "wait-on": "^8.0.3"
  },
  "overrides": {
    "vue": "latest"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/sitemap": "^7.2.10",
    "@nuxtjs/tailwindcss": "^6.13.2",
    "@simplewebauthn/browser": "^13.1.0",
    "@simplewebauthn/server": "^13.1.1",
    "@tsndr/cloudflare-worker-jwt": "^3.1.4",
    "@types/youtube": "^0.1.0",
    "@vuestic/nuxt": "^1.0.21",
    "@vueuse/motion": "^3.0.3",
    "@yeger/vue-masonry-wall": "^5.0.18",
    "babel-eslint": "^10.1.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^9.33.0",
    "nuxt-proxy": "^0.4.1",
    "pinia": "^2.3.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "ress": "^5.0.2",
    "vue-demi": "^0.14.10"
  },
  "packageManager": "pnpm@10.2.1+sha512.398035c7bd696d0ba0b10a688ed558285329d27ea994804a52bad9167d8e3a72bcb993f9699585d3ca25779ac64949ef422757a6c31102c12ab932e5cbe5cc92"
}
